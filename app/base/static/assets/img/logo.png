    <button class="btn btn-primary" type="button" data-bs-toggle="" data-bs-target="#" aria-controls="">Events</button>

    <div class="container-fluid">

  
   <div id = "pops"></div>
</div>

</div>
</div>


<script>
$('#q').append('<p color="black">q1</p>')
